---
layout: root.hbs
---

{{>page-info}}

<div class="jumbotron">
    <div class="container">
        <h1>{{page.title}} <small>{{page.subtitle}}</small></h1>
        {{#if page.description}}
            <ul class="text-right list-unstyled">
                {{#each page.description}}
                <li>{{.}}</li>
                {{/each}}
            </ul>
        {{/if}}
    </div>
</div>

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            {{> body }}
        </div>
    </div>
</div>
